<% for(var i = 0, len = data.list.length; i <len; i++) { %>
    <% var l = data.list[i]; %>
      	<div class="musixiser" data-id="<%-l.name%>">
        	<img class="l-avatar" src="<%-l.largeAvatar%>">
        	<div class="info">
          		<p class="name"><%-l.name%></p>
          		<p class="listener-total"><%-l.currentNum%>人正在收听</p>
          		<img class="nationality" src="<%-l.nationality%>"></img>
        	</div>
      	</div>
<% } %>